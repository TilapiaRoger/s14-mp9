{{#if success_locker_manage_msg}}
<div id="locker-manage-success" class="row bg-success">
	<div class="col">
		<p class="font-weight-bold">{{success_locker_manage_msg}}</p>
	</div>
</div>
{{/if}}

{{#if fail_locker_manage_msg}}
<div id="locker-manage-error" class="row">
	<div class="col">
		<p class="font-weight-bold">{{fail_locker_manage_msg}}</p>
	</div>
</div>
{{/if}}


