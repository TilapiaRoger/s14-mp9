{{#if success_profile_msg}}
<div id="profile-success"  class="row bg-success">
	<div class="col">
		<p class="font-weight-bold">{{success_profile_msg}}</p>
	</div>
</div>

{{/if}}

{{#if fail_profile_msg}}
<div id="profile-error" class="row">
	<div class="col">
		<p class="font-weight-bold">{{fail_profile_msg}}</p>
	</div>
</div>

{{/if}}